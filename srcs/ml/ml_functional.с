#include "../../includes/fdf.h"
#include "../../includes/matrix_list.h"

t_matrix_list	*ml_new(void)
{
	t_matrix_list	*out;

	_NOTIS_N(out = (t_matrix_list*)malloc(sizeof(t_matrix_list)));
	out->z = 0.0;
	out->p.y = 0.0;
	out->p.x = 0.0;
	out->rgb = 0;
	out->y = NULL;
	out->x = NULL;
	return (out);
}
